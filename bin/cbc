#!/usr/bin/env node
function main() {
    const args = process.argv.slice(2)
    const jarPath = require("path").join(__dirname, 'cbc-2018.04.12.jar')
    const cmd = `java -jar ${jarPath} ${args.join(" ")}`
    console.log(cmd);
    const output = require("child_process").execSync(cmd)
    console.log(output)
}

main()